.panel.panel-default#new-procedure-panel
  .panel-heading{ style: 'background-color: orange;' }
    %h1.text-center= t '.title'
  .panel-body
    .container
      = render 'form'
      .row
        .col-sm-4
          = form_tag import_procedures_path, multipart: true do
            .panel.panel-default
              .panel-heading
                %h3.panel-title
                  %label{ for: "file" } 上傳目前追料表CSV檔
              .panel-body
                .col-sm-8
                  = file_field_tag "file"
                .col-sm-4
                  = submit_tag "上傳", :class => "btn btn-primary"
          